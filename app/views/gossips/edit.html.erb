<h1>Gossips#edit</h1>


<%= form_for @gossips do |f| %>
  <div class="form-group">
    <label>Titre </label>
    <%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <label>Contenu</label>
    <%= f.text_area :content, class: 'form-control' %>
  </div>
  <div class="form-group">
    <button type="submit"class="btn btn-primary">Modifier l'article</button>
  </div>

<% end %>



